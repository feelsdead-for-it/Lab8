#include <iostream>
using namespace std;
#include <cmath>
int main()
  {
int x;

	cin >> x;

	if ((x>1)&&(x<10))
	{
		if (x % 2 == 0)
		{
			cout << "1znach chet" << ends;
		}
		else
		{
			cout << "1znach nechet" << ends;
		}
	}
	
  if ((x>9)&&(x<100))
	{
		if (x % 2 == 0)
		{
			cout << "2znach chet" << ends;
		}
		else
		{
			cout << "2znach nechet" << ends;
		}
	}
  if((x>99)&&(x<999))
	{
			if (x % 2 == 0)
			{
				cout << "3znach chet " << ends;
			}
			else
			{
				cout << "3znach nechet " << ends;
			}
	}
	
}
