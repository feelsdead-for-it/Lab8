#include <iostream>
using namespace std;
#include <cmath>
int main()
 {
int a, b, c, d;
    
    cin >> a >> b >> c;
    
    if (a > b)
		{
		
		if (b > c)
			{
				d = a + b;
			}
		else
			{
				d = a + c;
			}

		}
		else
		{
			if (a > c)
			{
				d = b + a;
			}
			else
			{
				d = b + c;
			}

		}
	cout << d << ends;
	
 }
